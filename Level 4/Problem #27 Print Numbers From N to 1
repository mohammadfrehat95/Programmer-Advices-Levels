#include <iostream>
using namespace std;

int ReadNumber()
{
	int Number = 0;
	cout << "Please Enter N to print from N to 1" << endl;
	cin >> Number;

	return Number;
}

void PrintRangeFromNto1(int n)
{

	for (int i = n; i >= 1 ; i--)
	{
		cout << i << endl;
	}
}

int main()
{
	int N = ReadNumber();
	PrintRangeFromNto1(N);
}

