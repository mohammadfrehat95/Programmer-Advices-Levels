#include <iostream>
using namespace std;

void ReadNumber(int &Num1, int &Num2)
{
	cout << "Please Enter Num 1 " << endl;
	cin >> Num1;

	cout << "Please Enter Num 2 " << endl;
	cin >> Num2;
}

int MaxOf2Number(int Num1, int Num2)
{
	if (Num1 > Num2)
		return Num1;
	else
		return Num2;
}

void PrintResult(int Num1, int Num2)
{
	cout << "Max of Two Number is: " << MaxOf2Number(Num1, Num2) << endl;
}

int main()
{
	int Num1, Num2;
	ReadNumber(Num1, Num2);
	MaxOf2Number(Num1, Num2);
	PrintResult(Num1, Num2);
}

