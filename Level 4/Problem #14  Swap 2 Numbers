#include <iostream>
using namespace std;

void ReadNumber(int &Num1, int &Num2)
{
	cout << "Please Enter Num 1 " << endl;
	cin >> Num1;

	cout << "Please Enter Num 2 " << endl;
	cin >> Num2;

}

int Swap2Number(int &Num1, int &Num2)
{
	int temp = Num1;
	Num1 = Num2;
	Num2 = temp;

	return Num1, Num2;
}

void PrintResult(int Num1, int Num2)
{
	cout << "\nBefore Swap\n";
	cout << "Num1: " << Num1 << endl;
	cout << "Num2: " << Num2 << endl;

	cout << "\nAfter Swap\n";
	Swap2Number(Num1, Num2);
	cout << "Num1: " << Num1 << endl;
	cout << "Num2: " << Num2 << endl;
}

int main()
{
	int Num1, Num2,Num3;
	ReadNumber(Num1, Num2);
	PrintResult(Num1, Num2);
}

